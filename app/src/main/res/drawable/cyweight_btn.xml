<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <!--填充颜色-->
            <solid android:color="@color/cadetblue"/>

            <!--矩形的圆角面积-->
            <stroke android:color="@color/gray" android:width="1dp" android:dashWidth="0dp"/>
            <corners  android:topRightRadius="6dp" android:bottomRightRadius="6dp"
                android:topLeftRadius="6dp" android:bottomLeftRadius="6dp"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <!--填充颜色-->
            <solid android:color="#ffffffff"/>

            <!--矩形的圆角面积-->
            <stroke android:color="@color/gray" android:width="1dp" android:dashWidth="0dp"/>
            <corners  android:topRightRadius="6dp" android:bottomRightRadius="6dp"
                android:topLeftRadius="6dp" android:bottomLeftRadius="6dp"/>
        </shape>
    </item>


</selector>